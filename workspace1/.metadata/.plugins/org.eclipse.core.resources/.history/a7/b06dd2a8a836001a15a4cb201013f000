package projetBanque;

public class CompteSimple {
	private int num;
	private int solde;
	private final int cadeau = 20;
	private final int com = 1;
	
	public CompteSimple(int num, int solde) {
		this.num = num;
		this.solde = solde;
	}
	
	public CompteSimple(int num) {
		this.num = num;
		this.solde = cadeau;
	}
	
	
}
