package projetBanque;

public class CompteCB extends CompteSimple {
	private CB cb;

	public CompteCB(String nom, int solde, CB cb) {
		super(nom, solde);
		this.cb = cb;
	}
	
	public CompteCB(String nom, CB cb) {
		super(nom);
		this.cb = cb;
	}



	public String toString() {
		return "CompteCB [cb=" + cb + "]" + super.toString();
	}
	
	
}
