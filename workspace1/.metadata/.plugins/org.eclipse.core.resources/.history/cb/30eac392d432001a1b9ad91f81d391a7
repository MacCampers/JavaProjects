package projetEleve;

public class Prof {
	private String name;
	private int note;
	private Eleve[] tabEleve;

	public Prof(String name) {
		this.name = name;
	}

	public void setNote(int note) {
		this.note = note;
	}
	

	public void setEleve(Eleve[] tabEleve) {
		this.tabEleve = tabEleve;
	}

	public int getNote() {
		return note;
	}

}
