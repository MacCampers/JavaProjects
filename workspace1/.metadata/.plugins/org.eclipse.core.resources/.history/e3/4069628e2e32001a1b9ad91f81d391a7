package projetSingleton;

public class Personne {
	private String nom = "Dupond";
	private static personne = new Personne();

	    private Personne() {} // prevent outside construction

	public static Personne getInstance() {
		return instance; // no synchronization needed
	}

	public String toString() {
		return "Personne [nom=" + nom + "]";
	}
}
