package projetEleve;

public class Test {

	public static void main(String[] args) {
		test1();
	}
	
	static void test1() {
		Eleve[] e = {new Eleve("test1"), new Eleve("test2")};
		Eleve e1 = new Eleve("test3");
		Eleve e2 = new Eleve("test3");
		Prof p = new Prof("testProf");
		for(Eleve a: e)
			a.setProf(p);
