package projetImprimante;

public class Centraliseur {
	private static Centraliseur instance = new Centraliseur();
	private Imprimante[] tab = [new Imprimante("IP1"), new Imprimante("IP2")];

	private Centraliseur() {
		
	}

	public static Centraliseur getInstance(String message, String name) {
		if (instance == null)
			instance = new Imprimante(name, message, "IP1");
		else {
			instance = new Imprimante(name, message, "IP2");
			instance = null;
		}
		return instance;
	}
}
