package src.model;

public class Authentification {
	private String nom;
	private String message;

	public Authentification(String nom) {
		this.nom = nom;
		getMessage();
	}
	
	private void getMessage() {
		if (nom == "titi" || nom == "toto" || nom == "dupond")
			this.message = "OK";
		else
			this.message = "Erreur";
	}

	public String getNom() {
		return nom;
	}

}
