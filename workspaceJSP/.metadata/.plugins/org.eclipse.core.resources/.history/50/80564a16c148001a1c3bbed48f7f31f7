<h1>RECAP</h1>

<% 
String nom=request.getParameter("nom");
String prenom=request.getParameter("prenom");
int age = Integer.parseInt(request.getParameter("age"));
int ageMin = Integer.parseInt(request.getParameter(request.getParameter("ageMin"));
int ageMax = Integer.parseInt(request.getParameter("ageMax"));
int ageMajeur = Integer.parseInt(request.getParameter("ageMajeur"));
if (age > ageMin && age < ageMax) {
	status = (age < ageMajeur) ? "Mineur" : "Majeur";
}
String res= "Nom: " + nom + "\nPrénom: " + prenom + "\nAge: " + age + "\nStatus: " + status;
out.print(res);
%>

