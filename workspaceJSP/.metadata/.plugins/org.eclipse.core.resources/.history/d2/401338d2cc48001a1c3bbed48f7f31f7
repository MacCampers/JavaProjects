package model;

public class Personne {
	private String nom;
	private String prenom;
	private int age;
	private String status = "Mineur";

	public Personne(String nom, String prenom, int age) {
		super();
		this.nom = nom;
		this.prenom = prenom;
		this.age = age;
	}

	public String toString() {
		return "Personne [nom=" + nom + ", prenom=" + prenom + ", age=" + age + ", status=" + status + "]";
	}

}
